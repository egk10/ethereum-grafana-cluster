      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 4,
      "title": "Block Processing",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(nethermind_blocks[5m])",
