        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      }
    },
    {
      "id": 5,
      "title": "Transaction Pool",
      "type": "graph",
      "targets": [
        {
          "expr": "nethermind_txpool_pending_count",
          "legendFormat": "Pending Transactions"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      }
    }
