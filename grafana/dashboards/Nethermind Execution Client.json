          "mappings": [
            {
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                },
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              },
              "type": "value"
            }
          ]
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Current Block",
      "type": "stat",
      "targets": [
        {
          "expr": "nethermind_blocks",
          "legendFormat": "Block Height"
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Network Peers",
      "type": "stat",
      "targets": [
        {
          "expr": "ethereum_peer_count",
          "legendFormat": "Active Peers"
        }
